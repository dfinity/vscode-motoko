{
    "For Loop (Collection)": {
        "prefix": [
            "for-each"
        ],
        "body": [
            "for(${1:x} in ${2:iter}.vals()) {",
            "\t$0",
            "};"
        ],
        "description": "A for loop which iterates through a collection."
    },
    "For Loop (Range)": {
        "prefix": [
            "for-range"
        ],
        "body": [
            "for(${1:i} in Iter.range(0, ${2:count} - 1)) {",
            "\t$0",
            "};"
        ],
        "description": "A for loop over a numeric range."
    },
    "Simple Type": {
        "prefix": [
            "type"
        ],
        "body": [
            "type ${1:Name} = ${2:declaration};"
        ],
        "description": "Simple type declaration"
    },
    "Object Type": {
        "prefix": [
            "type-obj"
        ],
        "body": [
            "type ${1:Name} = {",
            "\t${2:attribute} : ${3:Type};",
            "};"
        ],
        "description": "an object type block"
    },
    "Type Variant": {
        "prefix": [
            "type-variant"
        ],
        "body": [
            "type ${1:Name} = {",
            "\t#${2:Attribute};",
            "};"
        ],
        "description": "a variant type block"
    },
    "Mutable Variable": {
        "prefix": "var",
        "body": [
            "var ${1} = ${2};"
        ],
        "description": "declares a mutable variable"
    },
    "Immutable Variable": {
        "prefix": "let",
        "body": [
            "let ${1} = ${2};"
        ],
        "description": "declares an immutable variable"
    },
    "Simple Function": {
        "prefix": [
            "func"
        ],
        "body": [
            "func ${1:name} (4{2:arg} : ${5:type}) : ${4:returnType} {",
            "\t${5};",
            "};"
        ],
        "description": "Simple function declaration"
    },
    "Actor Function": {
        "prefix": [
            "actor-func"
        ],
        "body": [
            "public func ${1:name} (4{2:arg} : ${5:type}) : async ${4:returnType} {",
            "\t${5};",
            "};"
        ],
        "description": "Public actor function declaration"
    },
    "Base Library Import": {
        "prefix": [
            "base-import"
        ],
        "body": [
            "import ${1:Module} \"mo:base/${1}\";"
        ]
    },
    "Switch Statement": {
        "prefix": [
            "switch"
        ],
        "body": [
            "switch($1) {",
            "\tcase($2) { $0 };",
            "\tcase($3) { };",
            "};"
        ]
    },
    "Switch Statement (Option)": {
        "prefix": [
            "switch-option"
        ],
        "body": [
            "switch($1) {",
            "\tcase(?$1) { $0 };",
            "\tcase(null) { };",
            "};"
        ]
    },
    "Switch Statement (Result)": {
        "prefix": [
            "switch-result"
        ],
        "body": [
            "switch($1) {",
            "\tcase(#ok($1)) { $0 };",
            "\tcase(#err(err)) { };",
            "};"
        ]
    },
    "Array Literal": {
        "prefix": [
            "array"
        ],
        "body": [
            "[$0]"
        ]
    },
    "Mutable Array Literal": {
        "prefix": [
            "var-array"
        ],
        "body": [
            "[var $0]"
        ]
    },
    "New Hash Map": {
        "prefix": [
            "new-hash-map"
        ],
        "body": [
            "let ${1:map} = HashMap.HashMap<${2:Text}, ${3:Text}>(0, ${2}.equal, ${2}.hash);"
        ]
    },
    "New TrieMap": {
        "prefix": [
            "new-trie-map"
        ],
        "body": [
            "let ${1:map} = TrieMap.TrieMap<${2:Text}, ${3:Text}>(0);"
        ]
    },
    "New RB Tree": {
        "prefix": [
            "new-rb-tree"
        ],
        "body": [
            "let ${1:map} = RBTree.RBTree<${2:Text}, ${3:Text}>(${2}.compare);"
        ]
    },
    "New Trie Set": {
        "prefix": [
            "new-trie-set"
        ],
        "body": [
            "let ${1:set} = TrieSet.empty<${1}>();"
        ]
    },
    "New List": {
        "prefix": [
            "new-list"
        ],
        "body": [
            "let ${1:list} = List.nil<${1}>();"
        ]
    },
    "Ok (Result)": {
        "prefix": [
            "ok"
        ],
        "body": [
            "#ok($0)"
        ]
    },
    "Error (Result)": {
        "prefix": [
            "err"
        ],
        "body": [
            "#err($0)"
        ]
    },
    "Array to Blob": {
        "prefix": [
            "array-2-blob"
        ],
        "body": [
            "Blob.fromArray(${1:array})"
        ]
    },
    "Blob to Array": {
        "prefix": [
            "blob-2-array"
        ],
        "body": [
            "Blob.toArray(${1:blob})"
        ]
    },
    "Array to Buffer": {
        "prefix": [
            "array-2-buffer"
        ],
        "body": [
            "Buffer.fromArray(${1:array})"
        ]
    },
    "Buffer to Array": {
        "prefix": [
            "buffer-2-array"
        ],
        "body": [
            "Buffer.toArray(${1:buffer})"
        ]
    },
    "Principal to Text": {
        "prefix": [
            "principal-2-text"
        ],
        "body": [
            "Principal.toText(${1:principal})"
        ]
    },
    "Text to Principal": {
        "prefix": [
            "text-2-principal"
        ],
        "body": [
            "Principal.fromText(${1:text})"
        ]
    },
    "Actor to Principal": {
        "prefix": [
            "actor-2-principal"
        ],
        "body": [
            "Principal.fromActor(${1:actor})"
        ]
    }
}
